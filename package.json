{
  "name": "jac-template-express-js",
  "version": "1.0.0",
  "description": "ExpressJs API template using clean architecture",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "npm run lint && node swagger.js && SET NODE_ENV=dev node --es-module-specifier-resolution=node index.js",
    "start:dev": "nodemon --experimental-specifier-resolution=node index.js",
    "dev": "SET NODE_ENV=development & nodemon --experimental-specifier-resolution=node index.js",
    "lint": "eslint --fix ./*.js",
    "test": "npm run test:unit && npm run test:e2e",
    "test:watch": "npm run test:unit && npm run test:e2e --watch=false",
    "test:unit": "jest -- ./test/unit-test/*",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "author": "JAC",
  "license": "ISC",
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "@aws-sdk/client-sqs": "^3.370.0",
    "aws-sdk": "^2.1292.0",
    "axios": "^1.2.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "helmet": "^6.0.1",
    "hsts": "^2.2.0",
    "install": "^0.13.0",
    "newrelic": "9.9.0",
    "npm": "^9.8.0",
    "pg": "^8.10.0",
    "sqs-consumer": "^7.2.2"
  },
  "devDependencies": {
    "eslint": "^8.35.0",
    "eslint-config-standard": "^17.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.5.0",
    "nodemon": "^2.0.20",
    "swagger-autogen": "^2.23.1",
    "swagger-ui-express": "^4.1.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.test\\.js$",
    "transform": {
      ".+\\.(t|j)s$": "js-jest"
    },
    "testPathIgnorePatterns": [],
    "coveragePathIgnorePatterns": [
      "./src/config/*",
      "./index.js"
    ],
    "collectCoverage": true,
    "coverageReporters": [
      "lcov",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
